
@if (ViewBag.listoblast != null)
{   

    <div slass="col-md-10">
        <select id="oblast" onchange="changeOblast()" class = "form-control">
            <option value=0>Выбрать</option>
            @foreach (var item in ViewBag.listoblast)
            {
                <option value=@item.id>@item.name</option>
            }
        </select>
    </div>
}
else if (ViewBag.listgorod != null){
    <div slass="col-md-10">
        <select data-val="true" id="gorod" name="gorod" class = "form-control">
            <option value=0>Выбрать</option>
            @foreach (var item in ViewBag.listgorod)
            {
                <option value=@item.id>@item.name</option>
            }
        </select>
    </div>
}
